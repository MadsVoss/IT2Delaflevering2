<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController">
   <children>
      <Button layoutX="280.0" layoutY="340.0" mnemonicParsing="false" onAction="#startTemperatur" onMousePressed="#startTemperatur" text="Start" />
       <LineChart fx:id="lineChart" prefHeight="340.0" prefWidth="600.0" title="Temperatur" AnchorPane.rightAnchor="0.0">
           <xAxis>
               <CategoryAxis fx:id="Tid" endMargin="60.0" label="Tid i sekunder" side="BOTTOM" startMargin="0.0" />
           </xAxis>
           <yAxis>
               <NumberAxis fx:id="Temperatur" label="Temperatur i grader" lowerBound="35.0" prefHeight="155.0" prefWidth="46.0" side="LEFT" upperBound="45.0" />
           </yAxis>
       </LineChart>

   </children>
</AnchorPane>
